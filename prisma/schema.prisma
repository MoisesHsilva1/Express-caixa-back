generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Tenant {
    id        String   @id
    email     String   @unique
    name      String
    createdAt DateTime @default(now())
}

model Transaction {
    id  Int  @default(autoincrement()) @id
    tenant_id String
    description String
    amount Int
    type String
    transaction_date DateTime @default(now())
}